{
  "manifest_version": 2,
  "name": "tinder",
  "version": "0.1",
  "content_security_policy": "script-src 'self'; object-src 'self'",
  
  "content_scripts": [
    {
      "matches": [
        "https://tinder.com/app/matches",
        "https://tinder.com/app/recs"
      ],
      "css": ["style.css"],
      "js": ["content.js"]
    }
  ]
}
